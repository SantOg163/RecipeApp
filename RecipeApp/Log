SELECT [t].[RecipeId], [t].[Name], [t].[Method], [i].[Name], [i].[Quantity], [i].[Unit], [i].[IngredientId]
FROM (
    SELECT TOP(2) [r].[RecipeId], [r].[Name], [r].[Method]
    FROM [Recipes] AS [r]
    WHERE ([r].[RecipeId] = @__id_0) AND ([r].[IsDeleted] = CAST(0 AS bit))
) AS [t]
LEFT JOIN [Ingredient] AS [i] ON [t].[RecipeId] = [i].[RecipeId]
ORDER BY [t].[RecipeId]
